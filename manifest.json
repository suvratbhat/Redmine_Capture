{
    "name": "ENOVA - Report Redmine issues.",
    "version": "2.0.0",
    "manifest_version": 2,
    "description": "This extension allow you to report an issue from any page to Redmine.",
    "icons": {
        "16": "icon16.png",
        "48": "icon48.png",
        "128": "icon128.png"
    },

    "browser_action": {
        "default_icon": "icon.png",
        "default_title": "Send to Redmine",
        "default_popup": "popup.html"
    },
    "permissions": [
        "tabs",
        "activeTab",
        "http://*/*"
    ],
    "options_page": "options.html"
}